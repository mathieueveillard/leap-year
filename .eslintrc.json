{
  "env": {
    "browser": true,
    "es2021": true,
    "jest/globals": true
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint", "jest"],
  "rules": {
    "arrow-body-style": ["off"],
    "import/no-unresolved": ["off"],
    "max-len": ["off"],
    "quotes": ["error", "double"],
    "operator-linebreak": ["off"]
  }
}
